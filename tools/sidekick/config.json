{
    "project": "ClarkCountyNV",
    "plugins": [
        {
            "id": "library",
            "title": "Library",
            "environments": [
                "dev",
                "edit",
                "preview"
            ],
            "url": "/tools/sidekick/library.html",
            "includePaths": [
                "**.docx**"
            ]
        },
        {
            "id": "asset-library",
            "title": "AEM Assets Library",
            "environments": [
                "edit",
                "prod",
                "dev"
            ],
            "url": "https://experience.adobe.com/solutions/CQ-assets-selectors/static-assets/resources/franklin/asset-selector.html?hideTreeNav=false&rail=false&extConfigUrl=https://raw.githubusercontent.com/aemsites/clarkcountynv/refs/heads/main/tools/assets-selector/config.json",
            "isPalette": true,
            "passConfig": true,
            "includePaths": [
                "**.docx**"
            ],
             "copyMode": [
                 {
          "mimeType": "image/*",
          "value": "reference"
             }
            ],
       "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:1200px; height: calc(100vh - 60px)"
        }
    ]
}